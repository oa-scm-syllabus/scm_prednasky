{
  "name": "scm_prednasky",
  "description": "Přednášky SCM",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "zx ./scripts/build.js",
    "postbuild": "node scripts/build-index.js",
    "buildindex": "node scripts/build-index.js",
    "clean": "node ./scripts/clean.js",
    "export": "pnpm -r run export --with-clicks --wait-until none"
