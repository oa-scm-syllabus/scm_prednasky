    "@iconify-json/mdi": "^1.2.3",
    "fs-extra": "^11.3.1",
    "playwright": "^1.55.0",
    "playwright-chromium": "^1.55.0",
    "plop": "^4.0.1",
    "rimraf": "^6.0.1",
    "shiki": "^3.12.0",
    "zx": "^8.8.1"
  },
  "keywords": [
    "scm",
    "git",
    "github",
    "pva",
    "prednasky",
    "vyuka",
